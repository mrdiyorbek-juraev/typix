{
  "name": "@typix-editor/extension-short-cuts",
  "version": "4.1.0",
  "description": "Keyboard shortcuts extension for Typix",
  "keywords": [
    "typix",
    "editor",
    "shortcuts",
    "keyboard",
    "hotkeys"
  ],
  "author": "Diyorbek Juraev <mrdiyorbekjuraev@gmail.com>",
  "homepage": "https://typix.com",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrdiyorbek-juraev/typix.git"
  },
