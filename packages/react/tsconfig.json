{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "baseUrl": ".",
        "plugins": [
            {
                "name": "next"
            }
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}